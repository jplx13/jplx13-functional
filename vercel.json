{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "jplx13",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "functions": {
    "app/page.tsx": {
      "maxDuration": 30
    }
  }
} 
